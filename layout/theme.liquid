<!doctype html>
<html>
<head>
  <title>{{ page_title }}</title>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="{{ page_description | escape }}">
  <link rel="canonical" href="{{ canonical_url }}">
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins -->
  {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
  {{ 'fonts.css' | asset_url | stylesheet_tag }}
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}
  {{ 'vendor.js' | asset_url | script_tag }}
  <!-- {{ 'theme.js' | asset_url | script_tag }} -->
</head>
<body>

  {% section 'header' %}

  <main>

  {{ content_for_layout }}

  </main>

  {% section 'footer' %}


  <!-- {{ 'main.js' | asset_url | script_tag }} -->
  <!-- <script src="{{ 'jquery.min.js' | asset_url }}"></script> -->
<script src="{{ 'popper.min.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'bootstrap.min.js' | asset_url }}" defer="defer"></script>
<!-- <script src='//unpkg.com/spritespin@4.0.3/release/spritespin.js' type='text/javascript'></script> -->
<script src="{{ 'main.js' | asset_url }}" defer="defer"></script>
<script src="{{ 'video.js' | asset_url }}" defer="defer"></script>

</body>
</html>
